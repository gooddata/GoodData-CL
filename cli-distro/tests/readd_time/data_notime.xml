<schema>
    <name>data</name>
    <columns>
        <column>
            <name>date</name>
            <title>Date</title>
            <ldmType>DATE</ldmType>
            <datetime>false</datetime>
            <format>MM/dd/yyyy</format>
            <schemaReference>Date</schemaReference>
        </column>
        <column>
            <name>number</name>
            <title>Number</title>
            <ldmType>FACT</ldmType>
        </column>
        <column>
            <name>state</name>
            <title>State</title>
            <ldmType>ATTRIBUTE</ldmType>
        </column>
    </columns>
</schema>
